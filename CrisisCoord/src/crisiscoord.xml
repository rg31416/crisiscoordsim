<dsol:experimentalFrame 
	xmlns:dsol="http://www.simulation.tudelft.nl"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<experiment>
	  <model>
		   <model-class>nl.tudelft.simulation.crisiscoord.CrisisCoordModel</model-class>
